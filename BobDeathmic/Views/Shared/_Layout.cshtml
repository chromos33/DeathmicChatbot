<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - BobDeathmic</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/lib/FontAwesome/css/all.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/paddings.css" />
    <link rel="stylesheet" href="~/css/margins.css" />
    <link rel="stylesheet" href="~/css/settings.css" />
    <link rel="stylesheet" href="~/css/typography.css" />


</head>
<body>
    @await Html.PartialAsync("_Navi")
    <div class="body">
        @RenderBody()
    </div>

    <footer class="d-flex">
        @await Html.PartialAsync("_LoginPartial")
        @await Html.PartialAsync("_LogoutPartial")
        <a href="~/Datenschutz.html">Datenschutz</a>
    </footer>
    
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/responsivetables.js" asp-append-version="true"></script>
    <script src="~/js/jquery.validate.min.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)


</body>
</html>
